{
  "name": "varese",
  "version": "1.0.0",
  "description": "pitch space calculations",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "test-just": "mocha --compilers js:babel-register test/TestEnvironmentSetup.js -R spec",
    "unit-test": "mocha --compilers js:babel-register test/TestEnvironmentSetup.js 'test/**/*.@(js|jsx)' -R dot",
    "lint": "node node_modules/eslint/bin/eslint.js src test --ext '.js,.jsx'",
    "coverage": "babel-node ./node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- test/TestEnvironmentSetup.js 'test/**/*.@(js|jsx)' -R dot",
    "travis-coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "test": "npm run lint && npm run unit-test"
  },
  "babel": {
    "ignore": [
      "node_modules"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  },
  "keywords": [
    "var√®se"
  ],
  "author": "William Chargin",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.15",
    "babel-istanbul": "^0.5.9",
    "babel-loader": "^6.2.0",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "jsdom": "^3.1.2",
    "merge": "^1.2.0",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "history": "1.17.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "1.0.3",
    "redux": "^3.0.4",
    "vexflow": "^1.2.36"
  }
}
